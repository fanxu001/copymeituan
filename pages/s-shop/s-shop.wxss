.dialog-info-box{
  padding: 20rpx 30rpx ;
  text-align: left;
  color: #F4AD25;
  letter-spacing: 2rpx;
}
.options-box{
  padding: 40px 14px;
}
.option-list{
  width: 308px;
height: 106px;
background: #ffffff;
border-radius: 15px;
border:1px solid #F4AD25;
box-shadow: 0px 2px 2px 2px rgba(251,202,91,0.08); 
margin: 30px auto;
}
.option-item{
  display: flex;
  padding: 18px 24px;
}
.option-pic{
  width: 76px;
  height: 76px;
  flex-shrink: 0;
  padding-right: 20px;
}
.titles{
  flex: 2;
}
.title-biglit{
font-size: 17px;
font-weight: bold;
text-align: center;
color: #fccb5e;
}
.title-litlit{
font-size: 12px;
font-weight: bold;
text-align: center;
color: #cdcdcd;
margin-top: 14px;
}